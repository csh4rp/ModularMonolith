{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Messaging": {
    "Provider": "Postgres",
    "RunConsumers": true
  },
  "DataAccess": {
    "Provider": "Postgres"
  },
  "Modules": {
    "CategoryManagement": {
      "Enabled": true
    },
    "Identity": {
      "PermissionClaimName": "role",
      "Enabled": true
    }
  }
}
